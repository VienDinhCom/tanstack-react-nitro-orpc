{
  "name": "tanstack-react-nitro-orpc",
  "type": "module",
  "version": "0.0.0",
  "private": true,
  "homepage": "https://github.com/viendinhcom/esmate",
  "scripts": {
    "dev": "esmate task dev",
    "build": "esmate task build",
    "start": "esmate task start",
    "deploy": "esmate task deploy",
    "test": "esmate task test",
    "check": "esmate task check",
    "fix": "esmate task fix"
  },
  "dependencies": {
    "@esmate/react": "^3.1.5",
    "@esmate/shadcn": "^3.6.0",
    "@esmate/utils": "^1.3.4",
    "@orpc/client": "^1.13.4",
    "@orpc/server": "^1.13.4",
    "@orpc/tanstack-query": "^1.13.4",
    "@t3-oss/env-core": "^0.13.10",
    "@tanstack/react-devtools": "^0.9.4",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-query-devtools": "^5.91.3",
    "@tanstack/react-router": "^1.157.18",
    "@tanstack/react-router-devtools": "^1.157.18",
    "better-auth": "^1.4.18",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "tailwindcss": "^4.1.18",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20260131.0",
    "@esmate/eslint": "^1.3.0",
    "@esmate/prettier": "^1.0.6",
    "@tailwindcss/vite": "^4.1.18",
    "@tanstack/router-plugin": "^1.157.18",
    "@types/node": "^24.10.9",
    "@types/react": "^19.2.10",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9.39.2",
    "esmate": "^2.2.1",
    "jsdom": "^27.4.0",
    "nitro": "latest",
    "prettier": "^3.8.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-tsconfig-paths": "^6.0.5",
    "vitest": "^4.0.18",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.61.1"
  },
  "tasks": {
    "migrate": "wrangler d1 migrations apply tanstack-react-nitro-orpc $1",
    "types": "wrangler types src/backend/lib/types/cloudflare.d.ts",
    "dev": [
      "esmate task types",
      "esmate task migrate --local",
      "vite"
    ],
    "start": [
      "esmate task build",
      "esmate task migrate --local",
      "npx wrangler --cwd .output/ dev"
    ],
    "deploy": [
      "esmate task build",
      "esmate task migrate --remote",
      "npx wrangler --cwd .output/ deploy"
    ],
    "build": [
      "esmate task types",
      "tsc --noEmit",
      "vite build"
    ],
    "fix": [
      "esmate fmt",
      "esmate lint --fix"
    ],
    "check": [
      "tsc --noEmit",
      "esmate fmt --check",
      "esmate lint"
    ],
    "test": "vitest run"
  }
}
